{
  "name": "teleadmin",
  "version": "1.0.0",
  "scripts": {
    "prettier:base": "prettier \"**/*.{js,ts{,x},yml,yaml,json,md}\"",
    "prettier:write": "yarn prettier:base --write",
    "prettier:check": "yarn prettier:base --check",
    "lint:check": "yarn eslint --max-warnings 0 --format stylish \"!(node_modules)/**/*.ts{,x}\"",
    "lint:write": "yarn lint:check --fix",
    "prepare": "husky install",
    "deploy:main-back:heroku": "git subtree push --prefix main-back heroku main"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --max-warnings 0 --format stylish --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{js,yml,yaml,json,md,ice}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@blueprintjs/eslint-plugin": "^0.3.2",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-eslint": "^10.1.0",
    "clang-format": "^1.5.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "eslint-plugin-unused-imports": "^1.1.0",
    "git-branch-is": "^4.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prettier-plugin-slice": "^0.1.1",
    "release-it": "^14.4.0",
    "shx": "^0.3.3",
    "typescript": "^4.1.5",
    "yarnhook": "^0.5.1"
  },
  "dependencies": {}
}
